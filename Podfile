# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'Caledonia' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Caledonia

	pod 'FBSDKCoreKit'
  	pod 'FBSDKLoginKit'
  	pod 'FBSDKShareKit'
	pod 'SDWebImage'
	pod 'TPKeyboardAvoiding'
	pod 'RSLoadingView'
	pod 'GoogleMaps'
	pod 'GooglePlacePicker'
  	pod 'GooglePlaces'
	pod 'AAViewAnimator'
	pod 'XLActionController/Youtube’
	pod 'Firebase/Core'
	pod 'Firebase/Database’
	pod 'Firebase/Auth'
	
		

  target 'CaledoniaTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'CaledoniaUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
